    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.8",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "*.{vue,js}": [
      "eslint --fix",
      "prettier --write"
